{
    "presets": ["next/babel"],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["./"],
                "alias": {
                    "@pages": "./src/pages",
                    "@layouts": "./src/layouts",
                    "components": "./src/components",
                    "@styles": "./src/styles",
                    "@services": "./src/services",
                    "@models": "./src/models"
                }
            }
        ]
    ]
}
